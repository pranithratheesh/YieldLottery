{
  "compilerOptions": {
    "typeRoots": ["./node_modules/@types",
      "./path/to/generated/types" ],
    "target": "es2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  }
}
